{
  "name": "openshape",
  "version": "0.1.0",
  "private": true,
  "main": "electron/main.js",
  "scripts": {
    "dev": "cd openshape && PORT=3003 npm run dev",
    "build": "cd openshape && npm run export",
    "start": "cd openshape && npm run start",
    "lint": "cd openshape && npm run lint",
    "electron-dev": "concurrently \"cd openshape && npm run dev -- -p 3003\" \"wait-on http://localhost:3003 && ELECTRON_START_URL=http://localhost:3003 electron .\"",
    "electron-pack": "npm run build && electron-builder",
    "electron-build": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "appId": "com.openshape.cad",
    "productName": "OpenShape CAD",
    "files": [
      "openshape/out/**/*",
      "electron/**/*",
      "package.json"
    ],
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "dependencies": {
    "@jscad/modeling": "^2.12.5",
    "@jscad/obj-serializer": "^2.1.20",
    "@jscad/regl-renderer": "^2.5.0",
    "@jscad/stl-serializer": "^2.1.20",
    "electron-is-dev": "^2.0.0",
    "electron-serve": "^1.2.0",
    "lucide-react": "^0.294.0",
    "next": "^14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "three": "^0.159.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "electron": "^27.1.3",
    "electron-builder": "^24.9.1",
    "eslint": "^8.55.0",
    "eslint-config-next": "^14.0.4",
    "wait-on": "^7.2.0"
  }
}
